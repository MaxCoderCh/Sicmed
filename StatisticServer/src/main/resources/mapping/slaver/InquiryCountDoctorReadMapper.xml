<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sicmed.statistic.mapper.slaver.InquiryCountDoctorReadMapper">
    <resultMap id="BaseResultMap" type="com.sicmed.statistic.entity.InquiryCountDoctor">
        <id column="id" jdbcType="VARCHAR" property="id"/>
        <result column="doctor_id" jdbcType="VARCHAR" property="doctorId"/>
        <result column="inquiry_status" jdbcType="VARCHAR" property="inquiryStatus"/>
        <result column="create_time" jdbcType="TIMESTAMP" property="createTime"/>
    </resultMap>
    <sql id="Base_Column_List">
        id, doctor_id, inquiry_status, create_time
    </sql>
    <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
        select
        <include refid="Base_Column_List"/>
        from inquiry_count_doctor
        where id = #{id,jdbcType=VARCHAR}
    </select>
    <select id="getInquiryCount" parameterType="java.lang.String" resultType="java.lang.Integer">
        select
        count(*)
        from inquiry_count_doctor
        where doctor_id = #{id,jdbcType=VARCHAR}
    </select>
</mapper>